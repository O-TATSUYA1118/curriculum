package study;

public class Calculator {
	//フィールド変数
	public int a = 10;
	public int b = 20;
	public int c = 30;

	//メソッドたちがおる
    protected static int plus(int a) {
        return a + 1;
    }
    protected static int plus(int a, int b) {
        return a + b;
    }
    protected static int plus(int a, int b, int c) {
        return a + b + c;
    }

}