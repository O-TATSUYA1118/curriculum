package study;

public class Calculator {
	//フィールド変数
	public int a = 10;
	public int b = 20;
	public int c = 30;

	//メソッドたちがおる
    protected static int plus(int a) {
        int one = a + 1;
        System.out.println("plusメソッドの引数が一つの場合:" + one);
		return one;
    }
    protected static int plus(int a, int b) {
        int two = a + b;
        System.out.println("plusメソッドの引数が二つの場合:" + two);
        return two;
    }
    protected static int plus(int a, int b, int c) {
        int three = a + b + c;
        System.out.println("plusメソッドの引数が三つの場合:" +three);
        return three;
    }
}